<%= form_with(model: @article, local: true) do |form| %>
    <div>
      <%= form.label :title %>
      <%= form.text_field :title %>
    </div>
  
    <div>
      <%= form.label :body %>
      <%= form.text_area :body %>
    </div>
  
    <div>
      <%= form.submit %>
    </div>
<% end %>

<%= link_to 'Mostrar', article_path(@article) %> |
<%= link_to 'Lista de articulos', articles_path %>
